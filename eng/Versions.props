<Project>
  <!--Package versions-->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>6.0.100-preview.5.21260.2</MicrosoftDotnetSdkInternalPackageVersion>
    <MicrosoftAndroidSdkWindowsPackageVersion>30.0.100-ci.main.2</MicrosoftAndroidSdkWindowsPackageVersion>
    <MicrosoftMacCatalystSdkPackageVersion>14.5.100-ci.main.733</MicrosoftMacCatalystSdkPackageVersion>
    <MicrosoftiOSSdkPackageVersion>14.5.100-ci.main.733</MicrosoftiOSSdkPackageVersion>
    <MicrosofttvOSSdkPackageVersion>14.5.100-ci.main.733</MicrosofttvOSSdkPackageVersion>
    <MicrosoftmacOSSdkPackageVersion>11.3.100-ci.main.733</MicrosoftmacOSSdkPackageVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Trim all characters after first `-` or `+` is encountered. -->
    <DotNetPreviewVersionBand Condition=" '$(DotNetPreviewVersionBand)' == '' ">$([System.Text.RegularExpressions.Regex]::Replace($(MicrosoftDotnetSdkInternalPackageVersion), `[-+].*$`, ""))</DotNetPreviewVersionBand>
  </PropertyGroup>
</Project>
