<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:maui"
             x:Class="maui.App">
    <Application.Resources>
        <!-- NOTE: if the template ever drastically changes, compare this to the template! -->
        <ResourceDictionary Source="Resources/Styles.xaml" />
        <!-- Everything to create in addition to the template -->
        <ResourceDictionary>
          <SolidColorBrush x:Key="ProfiledAot_AppThemeBinding" Color="{AppThemeBinding Default=Black}"/>
          <SolidColorBrush x:Key="ProfiledAot_OnIdiom" Color="{OnIdiom Default=Black}"/>
          <CollectionView x:Key="ProfiledAot_CollectionView" Margin="{AppThemeBinding Default=1,1,1,1}" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
