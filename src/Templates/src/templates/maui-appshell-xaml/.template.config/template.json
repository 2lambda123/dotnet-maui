{
    "$schema": "http://json.schemastore.org/template",
    "author": "Microsoft",
    "classifications": [
        "MAUI",
        "Android",
        "iOS",
        "macOS",
        "Mac Catalyst",
        "WinUI",
        "Tizen",
        "Xaml",
        "Code"
    ],
    "identity": "Microsoft.Maui.XamlAppShell.DOTNET_TFM_VERSION_VALUE",
    "groupIdentity": "Microsoft.Maui.XamlAppShell",
    "precedence": "DOTNET_TFM_VERSION_MAJOR_VALUE",
    "name": ".NET MAUI AppShell (XAML)",
    "description": "An appshell to enable navigation using XAML.",
    "shortName": "maui-appshell-xaml",
    "tags": {
        "language": "C#",
        "type": "item"
    },
    "primaryOutputs": [
        {
            "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
            "path": "NewAppshell1.xaml"
        },
        {
            "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
            "path": "NewAppshell1.xaml.cs"
        }
    ],
    "postActions": [
        {
            "id": "openInEditor",
            "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
            "description": "Opens NewAppshell1.xaml in the editor.",
            "manualInstructions": [],
            "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EE6",
            "args": {
                "files": "0"
            },
            "continueOnError": true
        }
    ],
    "sourceName": "MauiAppShell1",
    "defaultName": "MauiAppShell1",
    "symbols": {
        "namespace": {
            "description": "namespace for the generated code",
            "replaces": "MauiAppShell1",
            "type": "parameter"
        }
    }
}
